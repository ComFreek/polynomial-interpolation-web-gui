{
  "name": "polynomial-interpolation-web-gui",
  "version": "0.0.1",
  "private": true,
  "description": "Polynomial Interpolation Web GUI with GeoGebra",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "compare-versions": "^3.4.0",
    "cypress": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-cypress": "^2.2.1",
    "http-server": "^0.11.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7"
  },
  "scripts": {
    "build": "npm run lint && npm run types && npm run stop-only",
    "start": "http-server app -c-1",
    "start:ci": "http-server app -c-1 --silent",
    "dev": "npm start -- -o",
    "test": "npm start & cypress run",
    "pretest": "npm run lint && npm run types",
    "lint": "eslint *.js cypress/**/*.js",
    "e2e": "cypress run",
    "test:ci": "run-p --race start:ci e2e",
    "cy:verify": "cypress verify",
    "cy:version": "cypress version",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "local:open": "start-test 8080 cy:open",
    "local:run": "start-test 8080 cy:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ComFreek/polynomial-interpolation-web-gui.git"
  },
  "author": {
    "name": "ComFreek",
